	langid = {english},
	note = {00022},
	keywords = {bibliometrics, dynamic, ranking},
	file = {Snapshot:/Users/jason/Library/Application Support/Firefox/Profiles/gvd79agc.default/zotero/storage/K9WVN5G6/West et al. - 2013 - Author-level Eigenfactor metrics Evaluating the i.html:text/html;West_et_al-2013-Journal_of_the_American_Society_for_Information_Science_and_Technology.pdf:/Users/jason/Library/Application Support/Firefox/Profiles/gvd79agc.default/zotero/storage/6ZZ23DFM/West_et_al-2013-Journal_of_the_American_Society_for_Information_Science_and_Technology.pdf:application/pdf}
