@InProceedings{Archambault2007Grouse,
  abstract = {Grouse is a feature-based approach to steerable exploration of a graph and an associated hierarchy. Steerability allows exploration to begin immediately, rather than requiring a costly layout of the entire graph as an initial step. In a feature-based approach, the subgraph inside a metanode of the graph hierarchy is laid out with a well- chosen algorithm appropriate for its topological structure. Grouse preserves the input hierarchy, which provides meaningful information to the user when its metanodes correspond to features of interest. When a metanode in the hierarchy is opened, a limited number of metanodes are laid out again along the path between the opened node and the root. We demonstrate the effectiveness of Grouse on datasets from IMDB, the Internet Movie Database, where nodes are actors and cliques represent movies. The combination of feature-based layout and limited relayout computation does not fragment features in the hierarchy and improves the number of levels in the hierarchy that can be seen at once over previous approaches.},
  acmid = {2384190},
  author = {Archambault, Daniel and Munzner, Tamara and Auber, David},
  booktitle = {Proceedings of the 9th Joint Eurographics / IEEE VGTC Conference on Visualization},
  doi = {10.2312/VisSym/EuroVis07/067-074},
  isbn = {978-3-905673-45-6},
  keywords = {publication_channel:EuroVis, paper_type:technique, group_type:vertex-based, graph_vis:node-link, group_overlap:disjoint, group_structure:hierarchical, navigation, aggregation, implicit_vis:2D_layout, edge-group_vis:none, group_vis:embedded, implicit_vis:2d_meta_layout, group_origin:topology_clustering, graph:generic, evaluation:comparison, feature-based_layout, application:generic, superimposed_vis:!, juxtaposed_vis:!, visual_attr:!, embedded_vis:node-link},
  numpages = {8},
  pages = {67--74},
  publisher = {Eurographics Association},
  references = {Abello2004Compound-Fisheye Archambault2007TopoLayout Abello2006ASK-GraphView Eades2000NavClustG Schaffer1996NavHierClust vanHam2004SmallWorldG},
  series = {EuroVis '07},
  title = {Grouse: Feature-based, Steerable Graph Hierarchy Exploration},
  year = {2007}
}

@Article{Archambault2007TopoLayout,
  abstract = {We describe TopoLayout, a feature-based, multilevel algorithm that draws undirected graphs based on the topological features they contain. Topological features are detected recursively inside the graph, and their subgraphs are collapsed into single nodes, forming a graph hierarchy. Each feature is drawn with an algorithm tuned for its topology. As would be expected from a feature- based approach, the runtime and visual quality of TopoLayout depends on the number and types of topological features present in the graph. We show experimental results comparing speed and visual quality for TopoLayout against four other multilevel algorithms on a variety of data sets with a range of connectivities and sizes. TopoLayout frequently improves the results in terms of speed and visual quality on these data sets.},
  author = {Archambault, Daniel and Munzner, Tamara and Auber, David},
  doi = {10.1109/TVCG.2007.46},
  journal = {IEEE Transactions on Visualization and Computer Graphics},
  keywords = {paper_type:technique_layout, group_type:vertex-based,, graph_vis:node-link, group_overlap:disjoint, group_structure:hierarchical, publication_channel:TVCG, graph:generic, aggregation, group_origin:topology_clustering, implicit_vis:2D_layout, evaluation:comparison, application:generic, edge-group_vis:none, group_vis:implicit, implicit_vis:2d_meta_layout, force-directed_layout, circular_layout, tree_layout, gem_layout, superimposed_vis:!, juxtaposed_vis:!, visual_attr:!, embedded_vis:!},
  number = {2},
  pages = {305--317},
  references = {Abello2004Compound-Fisheye Auber2003MScaleVisNW vanHam2004SmallWorldG},
  title = {{TopoLayout}: Multilevel Graph Layout by Topological Features},
  volume = {13},
  year = {2007}
}

@Article{Archambault2008GrouseFlocks,
  abstract = {Several previous systems allow users to interactively explore a large input graph through cuts of a superimposed hierarchy. This hierarchy is often created using clustering algorithms or topological features present in the graph. However, many graphs have domain-specific attributes associated with the nodes and edges which could be used to create many possible hierarchies providing unique views of the input graph. GrouseFlocks is a system for the exploration of this graph hierarchy space. By allowing users to see several different possible hierarchies on the same graph, the system helps users investigate graph hierarchy space instead of a single, fixed hierarchy. GrouseFlocks provides a simple set of operations so that users can create and modify their graph hierarchies based on selections. These selections can be made manually or based on patterns in the attribute data provided with the graph. It provides feedback to the user within seconds, allowing interactive exploration of this space.},
  author = {Archambault, Daniel and Munzner, Tamara and Auber, David},
  doi = {10.1109/TVCG.2008.34},
  journal = {IEEE Transactions on Visualization and Computer Graphics},
  keywords = {paper_type:technique, group_type:vertex-based, group_overlap:disjoint, group_structure:hierarchical, graph_vis:node-link, graph:multivariate, group_origin:generic, group_origin:interaction, superimposed_vis:!, edge-group_vis:none, group_vis:embedded, evaluation:user_feedback, evaluation:case_study, application:media, application:social_network, co-author, aggregation, publication_channel:TVCG, implicit_vis:!, juxtaposed_vis:!, visual_attr:!, embedded_vis:node-link},
  number = {4},
  pages = {900--913},
  references = {Abello2004Compound-Fisheye Abello2006ASK-GraphView Archambault2007Grouse Archambault2007TopoLayout Balzer2007LoD-clust diGiacomo2007WebClust Eades2000NavClustG Pattison2001ComposableLayout Pretorius2006MultivariateStateTransitionG Schaffer1996NavHierClust Shneiderman2006SemanticSubstrates vanHam2004SmallWorldG},
  title = {{GrouseFlocks}: Steerable Exploration of Graph Hierarchy Space},
  volume = {14},
  year = {2008}
}

@InProceedings{Archambault2009TugGraph,
  abstract = {Many graph visualization systems use graph hierarchies to organize a large input graph into logical components. These approaches detect features globally in the data and place these features inside levels of a hierarchy. However, this feature detection is a global process and does not consider nodes of the graph near a feature of interest. TugGraph is a system for exploring paths and proximity around nodes and subgraphs in a graph. The approach modifies a pre-existing hierarchy in order to see how a node or subgraph of interest extends out into the larger graph. It is guaranteed to create path-preserving hierarchies, so that the abstraction shown is meaningful with respect to the structure of the graph. The system works well on graphs of hundreds of thousands of nodes and millions of edges. TugGraph is able to present views of this proximal information in the context of the entire graph in seconds, and does not require a layout of the full graph as input.},
  author = {Archambault, Daniel and Munzner, Tamara and Auber, David},
  booktitle = {Proceedings of the IEEE Pacific Visualization Symposium},
  doi = {10.1109/PACIFICVIS.2009.4906845},
  keywords = {paper_type:technique, group_type:vertex-based, group_overlap:disjoint, group_structure:hierarchical, graph_vis:node-link, edge-group_vis:none, group_vis:embedded, superimposed_vis:!, graph:generic, group_origin:topology_clustering, evaluation:case_study, application:economy, application:computer_network, application:media, aggregation, navigation, juxtaposed_vis:!, publication_channel:PacificVis, implicit_vis:!, visual_attr:!, embedded_vis:node-link},
  pages = {113--120},
  publisher = {IEEE},
  references = {Abello2004Compound-Fisheye Abello2006ASK-GraphView Archambault2007Grouse Archambault2008GrouseFlocks Balzer2007LoD-clust diGiacomo2007WebClust Eades1996MVCG Eades2000NavClustG Pattison2001ComposableLayout Schaffer1996NavHierClust vanHam2004SmallWorldG},
  series = {PacificVis '09},
  title = {{TugGraph}: Path-preserving hierarchies for browsing proximity and paths in graphs},
  year = {2009}
}

