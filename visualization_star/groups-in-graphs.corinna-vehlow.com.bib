  isbn = {978-3-642-37455-5},
  keywords = {paper_type:technique, graph_vis:node-link, group_type:edge-based, group_overlap:overlapping_crisp, group_structure:flat, edge-group_vis:visual_attr, group_vis:none, visual_attr:color, juxtaposed_vis:!, superimposed_vis:!, embedded_vis:!, implicit_vis:!, graph:directed, graph:multi, group_origin:generic, evaluation:case_study, application:social_network, publication_channel:other_collection},
  language = {English},
  pages = {49-60},
  publisher = {Springer},
  series = {Lecture Notes in Computer Science},
  title = {{ViStruclizer}: A Structural Visualizer for Multi-dimensional Social Networks},
  volume = {7819},
  year = {2013}
}

@InProceedings{Daniel2014Polyptychon,
  abstract = {Architects and developers are often tasked with evaluating or maintaining unfamiliar software systems. Reverse engineering tools help extract relationships between the system parts as they exist instead of as documented. Though node-link diagrams have a straightforward correspondence with the graph- representable data generated, the scale and complexity of real- world data sets prevent efficient comprehension. This paper presents Polyptychon, an interactive node-link visualization designed for incremental exploration of dependency information. Given a hierarchical information space of software artifacts, Polyptychon constrains the visible dependencies to be related to the child nodes of a specified artifact node, called a view root . It then classifies these siblings as levelized, tangled and independent. It also includes context nodes, which are a filtered set of nodes elsewhere in the hierarchy that are related to the siblings. The context nodes are further grouped based on a project-specific partition function. The hierarchical constraints and partition function provide means to control the number of nodes displayed, while the dependency classification allows users to form a qualitative impression of the dependency structure. We demonstrate with examples from the Netty open source project. We conclude with areas of future work, in particular, as a basis of evolutionary dependency analysis.},
  author = {Daniel, Donny T. and Wuchner, Egon and Sokolov, Konstantin and Stal, Michael and Liggesmeyer, Peter},
  booktitle = {Proceedings of the 2nd IEEE Working Conference on Software Visualization},
  doi = {10.1109/VISSOFT.2014.23},
  keywords = {publication_channel:VISSOFT,paper_type:technique, group_type:vertex-based, graph_vis:node-link, group_overlap:disjoint, group_structure:hierarchical, group_origin:generic, edge-group_vis:none, group_vis:superimposed, superimposed_vis:partitioning, evaluation:case_study, application:software_engineering, implicit_vis:!, juxtaposed_vis:!, graph:generic, visual_attr:!, embedded_vis:!},
  pages = {83--86},
  publisher = {IEEE},
  references = {Sangal2005DepModel Pattison2001ComposableLayout Holten2006HierEdgeBundles Archambault2007Grouse},
  series = {VISSOFT '14},
  title = {Polyptychon: A Hierarchically-constrained Classified Dependencies Visualization},
  year = {2014}
}

@Article{Davis2011DiseaseNW,
  abstract = {The availability of electronic health care records is unlocking the potential for novel studies on understanding and modeling disease co-morbidities based on both phenotypic and genetic data. Moreover, the insurgence of increasingly reliable phenotypic data can aid further studies on investigating the potential genetic links among diseases. The goal is to create a feedback loop where computational tools guide and facilitate research, leading to improved biological knowledge and clinical standards, which in turn should generate better data. We build and analyze disease interaction networks based on data collected from previous genetic association studies and patient medical histories, spanning over 12 years, acquired from a regional hospital. By exploring both individual and combined interactions among these two levels of disease data, we provide novel insight into the interplay between genetics and clinical realities. Our results show a marked difference between the well defined structure of genetic relationships and the chaotic co-morbidity network, but also highlight clear interdependencies. We demonstrate the power of these dependencies by proposing a novel multi-relational link prediction method, showing that disease co-morbidity can enhance our currently limited knowledge of genetic association. Furthermore, our methods for integrated networks of diverse data are widely applicable and can provide novel advances for many problems in systems biology and personalized medicine.},
  author = {Davis, Darcy A. AND Chawla, Nitesh V.},
  doi = {10.1371/journal.pone.0022670},
  journal = {PLoS ONE},
  keywords = {paper_type:application, group_type:vertex-based, paper_type:application, group_type:edge-based, graph_vis:node-link, group_overlap:disjoint, group_overlap:overlapping_crisp, group_structure:flat, edge-group_vis:none, group_vis:visual_attr, visual_attr:color, juxtaposed_vis:!, superimposed_vis:!, embedded_vis:!, implicit_vis:!, graph:generic, group_origin:generic, evaluation:none, application:biology, publication_channel:other_journal},
  month = {07},
  number = {7},
  pages = {e22670},
  publisher = {Public Library of Science},
  title = {Exploring and Exploiting Disease Interactions from Multi-Relational Gene and Phenotype Networks},
  volume = {6},
  year = {2011}
}

@InProceedings{Dekker2001CAVALIER,
  abstract = {Social Network Analysis is an approach to analysing organisations focusing on relationships as the most important aspect. In this paper we discuss visualisation techniques for Social Network Analysis, including spring-embedding and simulated annealing techniques. We introduce a visualisation technique based on Kohonen neural networks, and also introduce social flow diagrams for demonstrating the relationship between two forms of conceptual distance.},
  author = {Dekker, Anthony H.},
