  references = {Abello2006ASK-GraphView Archambault2007TopoLayout Auber2003MScaleVisNW Eades1996MVCG Fekete2003LinkOnTreemap},
  series = {ICIG '07},
  title = {A Space Efficient Clustered Visualization of Large Graphs},
  year = {2007}
}

@InProceedings{Huang2007TripleLayer,
  abstract = {This paper presents a technique for navigating large clustered graphs with triple-layer display. Although our navigation and interaction technique can be applied independently to any graph layout algorithms, in our implementation, our navigation algorithm is cooperated with a clustered graph layout algorithm. In short, we provide users with the display of three levels of contextual information for navigation. They are the display of full-context, the display of the current-context view and the display of the focused view. Our technique not only allows users to navigate hierarchically up and down through the entire clustered graph, but also provides a way to navigate multiple clusters concurrently. We also use animation to preserve the users? mental maps during the interaction.},
  author = {Huang, Mao Lin and Nguyen, Quang Vinh},
  booktitle = {Proceedings of the 11th International Conference on Information Visualization},
  doi = {10.1109/IV.2007.83},
  keywords = {publication_channel:IV, group_overlap:disjoint, group_structure:hierarchical, navigation, edge-group_vis:none, group_vis:embedded, superimposed_vis:!, paper_type:technique, group_type:vertex-based, implicit_vis:2D_meta_layout, graph_vis:node-link, graph:generic, group_origin:topology_clustering, evaluation:none, application:generic, juxtaposed_vis:!, embedded_vis:node-link, visual_attr:!},
  pages = {684--692},
