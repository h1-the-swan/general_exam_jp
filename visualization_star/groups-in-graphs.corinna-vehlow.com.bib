  title = {Higres - Visualization System for Clustered Graphs and Graph Algorithms},
  volume = {1731},
  year = {1999}
}

@InProceedings{Liu2014Topicpanorama,
  abstract = {We present a visual analytics approach to developing a full picture of relevant topics discussed in multiple sources such as news, blogs, or micro-blogs. The full picture consists of a number of common topics among multiple sources as well as distinctive topics. The key idea behind our approach is to jointly match the topics extracted from each source together in order to interactively and effectively analyze common and distinctive topics. We start by modeling each textual corpus as a topic graph. These graphs are then matched together with a consistent graph matching method. Next, we develop an LOD-based visualization for better understanding and analysis of the matched graph. The major feature of this visualization is that it combines a radially stacked tree visualization with a density-based graph visualization to facilitate the examination of the matched topic graph from multiple perspectives. To compensate for the deficiency of the graph matching algorithm and meet different users’ needs, we allow users to interactively modify the graph matching result. We have applied our approach to various data including news, tweets, and blog data. Qualitative evaluation and a real-world case study with domain experts demonstrate the promise of our approach, especially in support of analyzing a topic-graph-based full picture at different levels of detail.},
  author = {Liu, Shixia and Wang, Xiting and Chen, Jianfei and Zhu, Jun and Guo, Baining},
  booktitle = {Proceedings of the Conference on Visual Analytics Science and Technology},
  doi = {10.1109/VAST.2014.7042494},
  keywords = {publication_channel:TVCG, paper_type:technique, group_type:vertex-based, group_overlap:overlapping_crisp, edge-group_vis:none, group_vis:visual_attr, visual_attr:color, pie_chart, graph_vis:node-link, implicit_vis:2D_layout, group_origin:categorical_attr, evaluation:case_study, graph:generic, application:social_network, group_structure:flat, superimposed_vis:!, juxtaposed_vis:!, visual_attr:glyph, embedded_vis:!},
  pages = {183--192},
  publisher = {IEEE},
  references = {Cao2010FacetAtlas Collins2007VisLink Hascoet2012ClusterComparison Henry2007NodeTrix Kumar2006DynGraph Vehlow2013Fuzzy},
  series = {VAST '14},
  title = {{TopicPanorama}: a full picture of relevant topics},
  volume = {20},
  year = {2014}
}

@TechReport{Mader2007PlanarCluster,
  abstract = {Clustering techniques have proven to be useful in reducing the com- plexity of large networks. We present a method for drawing a clustered graph in 2.5D, given the clusters of the graph are connected in a pla- nar way. The main contribution is to develop a weighted version of the well known 2D straight line drawing algorithm of de Fraysseix, Pach and Pollack [5] for planar graphs. This version allows for thick vertices and ensures a constraint we define as ``visibility'' between connected vertices. The algorithm has been implemented and experimentally evaluated.},
  author = {Mader, Martin and Hong, Seok-Hee},
