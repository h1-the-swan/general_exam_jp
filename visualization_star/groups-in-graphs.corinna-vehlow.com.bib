  doi = {10.7155/jgaa.00016},
  journal = {Journal of Graph Algorithms and Applications},
  keywords = {publication_channel:JGAA, paper_type:technique_layout, group_type:vertex-based,, graph_vis:node-link, group_overlap:disjoint, group_structure:hierarchical, group_origin:generic, edge-group_vis:none, group_vis:implicit, implicit_vis:2D_layout, superimposed_vis:contour, graph:directed, orthogonal_layout, implicit_vis:2D_layout_dummyV, evaluation:none, application:generic, juxtaposed_vis:!, visual_attr:!, embedded_vis:!},
  number = {4},
