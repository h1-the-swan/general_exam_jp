  pages = {201--208},
  publisher = {IEEE},
  references = {Fekete2003LinkOnTreemap Greilich2009TimeArcTrees Neumann2005ArcTrees},
  series = {PacificVis '09},
  title = {Visualizing edge-edge relations in graphs},
  year = {2013}
}

