  pages = {492--502},
  publisher = {Springer},
  references = {Eades1996MVCG Frishman2004Clust},
  series = {GD '05},
  title = {Drawing Clustered Graphs in Three Dimensions},
  volume = {3843},
  year = {2006}
}

