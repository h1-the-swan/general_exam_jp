  number = {3},
  pages = {303--320},
  references = {Frishman2004Clust},
  title = {{MOVIS}: A system for visualizing distributed mobile object environments},
  volume = {19},
  year = {2008}
}

@InCollection{Gansner2007EdgeBundles,
  abstract = {Circular graph layout is a drawing scheme where all nodes are placed on the perimeter of a circle. An inherent issue with circular layouts is that the rigid restriction on node placement often gives rise to long edges and an overall dense drawing. We suggest here three independent, complementary techniques for lowering the density and improving the readability of circular layouts. First, a new algorithm is given for placing the nodes on the circle such that edge lengths are reduced. Second, we enhance the circular drawing style by allowing some of the edges to be routed around the exterior of the circle. This is accomplished with an algorithm for optimally selecting such a set of externally routed edges. The third technique reduces density by coupling groups of edges as bundled splines that share part of their route. Together, these techniques are able to reduce clutter, density and crossings compared with existing methods.},
  author = {Gansner, Emden R. and Koren, Yehuda},
  booktitle = {Proceedings of the 15th International Symposium on Graph Drawing},
  doi = {10.1007/978-3-540-70904-6_37},
  keywords = {paper_type:technique, group_type:edge-based, graph_vis:node-link, group_overlap:disjoint, group_structure:flat, edge-group_vis:visual_attr, group_vis:none, visual_attr:color, superimposed_vis:!, juxtaposed_vis:!, embedded_vis:!, implicit_vis:edge-bundling, graph:generic, group_origin:topology_clustering, evaluation:algorithmic, application:generic, publication_channel:GD, edge-bundling, circular_layout},
  pages = {386-398},
  publisher = {Springer},
  references = {Dogrusoz1996CircularLayout},
  series = {GD '07},
  title = {Improved Circular Layouts},
  volume = {4372},
  year = {2007}
}

@InProceedings{Gansner2010GMap,
  abstract = {Information visualization is essential in making sense out of large data sets. Often, high-dimensional data are visualized as a collection of points in 2-dimensional space through dimensionality reduction techniques. However, these traditional methods often do not capture well the underlying structural information, clustering, and neighborhoods. In this paper, we describe GMap, a practical algorithm for visualizing relational data with geographic-like maps. We illustrate the effectiveness of this approach with examples from several domains.},
  author = {Gansner, Emden R. and Hu, Yifan and Kobourov, Stephen G.},
