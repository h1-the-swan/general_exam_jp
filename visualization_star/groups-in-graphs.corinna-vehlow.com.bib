  pages = {47-54},
  publisher = {IEEE},
  references = {Abello2006ASK-GraphView Alper2011LineSets Archambault2008GrouseFlocks Balzer2007LoD-clust Elmqvist2008ZAME Henry2006MatrixExplorer Jianu2014EvalGroupNL Muelder2008TreeMap HenryRiche2010Euler Shen2006OntoVis vanHam2004SmallWorldG Vehlow2013Fuzzy Yang2013PIWI},
  series = {PacificVis '15},
  title = {Interactive visual summary of major communities in a large network},
  year = {2015}
}

@InCollection{Xu2009GeneOntology,
  abstract = {In this paper we propose three layout algorithms for semi-bipartite graphs—bipartite graphs with edges in one partition—that emerge from microarray experiment analysis. We also introduce a method that effectively reduces visual complexity by removing less informative nodes. The drawing quality and running time are evaluated with five real-world datasets, and the results show significant reduction in crossing number and total edge length. All the proposed methods are available in visualization package GEOMI [1], and are well received by domain users.},
  author = {Xu, Kai and Williams, Rohan and Hong, Seok-Hee and Liu, Qing and Zhang, Ji},
