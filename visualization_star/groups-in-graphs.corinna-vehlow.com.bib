  pages = {452--463},
  publisher = {Springer},
  references = {Alper2011LineSets Dinkla2012KelpDiagrams Hu2010ClusterMaps Jianu2014EvalGroupNL HenryRiche2010Euler},
  series = {GD '14},
  title = {{MapSets}: Visualizing Embedded and Clustered Graphs},
  volume = {8871},
  year = {2014}
}

@Article{Elmqvist2007CiteWiz,
  abstract = {We present CiteWiz, an extensible framework for visualization of scientific citation networks. The system is based on a taxonomy of citation database usage for researchers, and provides a timeline visualization for overviews and an influence visualization for detailed views. The timeline displays the general chronology and importance of authors and articles in a citation database, whereas the influence visualization is implemented using the Growing Polygons technique, suitably modified to the context of browsing citation data. Using the latter technique, hierarchies of articles with potentially very long citation chains can be graphically represented. The visualization is augmented with mechanisms for parent–child visualization and suitable interaction techniques for interacting with the view hierarchy and the individual articles in the dataset. We also provide an interactive concept map for keywords and co-authorship using a basic force-directed graph layout scheme. A formal user study indicates that CiteWiz is significantly more efficient than traditional database interfaces for high-level analysis tasks relating to influence and overviews, and equally efficient for low-level tasks such as finding a paper and correlating bibliographical data.},
  author = {Elmqvist, Niklas and Tsigas, Philippas},
  doi = {10.1057/palgrave.ivs.9500156},
  journal = {Information Visualization},
  keywords = {publication_channel:InfoVis_J, application:social_network, evaluation:user_study, force-directed_layout, navigation, aggregation, graph:directed, paper_type:technique, group_type:vertex-based, graph_vis:node-link, group_overlap:disjoint, group_structure:flat, edge-group_vis:none, group_vis:superimposed, visual_attr:color, implicit_vis:1D_layout, evaluation:case_study, superimposed_vis:contour, contour_filled, group_origin:categorical_attr, juxtaposed_vis:!, embedded_vis:!},
  number = {3},
  pages = {215--232},
  title = {{CiteWiz}: a tool for the visualization of scientific citation networks},
  volume = {6},
  year = {2007}
}

@InProceedings{Elmqvist2008ZAME,
  abstract = {We present the zoomable adjacency matrix explorer (ZAME), a visualization tool for exploring graphs at a scale of millions of nodes and edges. ZAME is based on an adjacency matrix graph representation aggregated at multiple scales. It allows analysts to explore a graph at many levels, zooming and panning with interactive performance from an overview to the most detailed views. Several components work together in the ZAME tool to make this possible. Efficient matrix ordering algorithms group related elements. Individual data cases are aggregated into higher-order meta-representations. Aggregates are arranged into a pyramid hierarchy that allows for on-demand paging to GPU shader programs to support smooth multiscale browsing. Using ZAME, we are able to explore the entire French Wikipedia - over 500,000 articles and 6,000,000 links - with interactive performance on standard consumer-level computer hardware.},
  author = {Elmqvist, Niklas and Do, Thanh-Nghi and Goodell, Howard and Henry, Nathalie and Fekete, Jean-Daniel},
  booktitle = {Proceedings of the IEEE Pacific Visualization Symposium},
  doi = {10.1109/PACIFICVIS.2008.4475479},
  keywords = {group_overlap:disjoint, group_structure:hierarchical, graph_vis:matrix, aggregation, publication_channel:PacificVis, edge-group_vis:none, group_vis:implicit, implicit_vis:1D_layout, paper_type:technique_layout, group_type:vertex-based,, application:biology, application:social_network, superimposed_vis:!, graph:generic, group_origin:generic, evaluation:none, juxtaposed_vis:!, visual_attr:!, embedded_vis:!},
  pages = {215--222},
  publisher = {IEEE},
  references = {Auber2003MScaleVisNW Henry2006MatrixExplorer},
  series = {PacificVis '08},
  title = {{ZAME}: Interactive Large-Scale Graph Visualization},
  year = {2008}
}

@InProceedings{Erdemir2015EQuality,
  abstract = {Recently, with increasing maintenance costs, studies on software quality are becoming increasingly important and widespread because high quality software means more easily maintainable software. Measurement plays a key role in quality improvement activities and metrics are the quantitative measurement of software design quality. In this paper, we introduce a graph based object-oriented software quality visualization tool called "E-Quality". E-Quality automatically extracts quality metrics and class relations from Java source code and visualizes them on a graph-based interactive visual environment. This visual environment effectively simplifies comprehension and refactoring of complex software systems. Our approach assists developers in understanding of software quality attributes by level categorization and intuitive visualization techniques. Experimental results show that the tool can be used to detect software design flaws and refactoring opportunities.},
  author = {Erdemir, Ural and Tekin, Umut and Buzluca, Feza},
  booktitle = {Proceedings of the 6th IEEE International Workshop on Visualizing Software for Understanding and Analysis},
  doi = {10.1109/VISSOF.2011.6069454},
  keywords = {paper_type:technique, group_type:edge-based, graph_vis:node-link, group_overlap:disjoint, group_structure:flat, edge-group_vis:visual_attr, group_vis:none, visual_attr:color, juxtaposed_vis:!, superimposed_vis:!, embedded_vis:!, implicit_vis:!, graph:weighted, group_origin:categorical_attr, graph:directed, evaluation:none, application:software_engineering, publication_channel:VISSOFT},
  pages = {1-8},
  references = {Termeer2005Metric},
  series = {VISSOFT '11},
  title = {{E-Quality}: A graph based object oriented software quality visualization tool},
  year = {2011}
}

@Article{Ersoy2011SkeletonEdgeBundling,
  abstract = {In this paper, we present a novel approach for constructing bundled layouts of general graphs. As layout cues for bundles, we use medial axes, or skeletons, of edges which are similar in terms of position information. We combine edge clustering, distance fields, and 2D skeletonization to construct progressively bundled layouts for general graphs by iteratively attracting edges towards the centerlines of level sets of their distance fields. Apart from clustering, our entire pipeline is image-based with an efficient implementation in graphics hardware. Besides speed and implementation simplicity, our method allows explicit control of the emphasis on structure of the bundled layout, i.e. the creation of strongly branching (organic-like) or smooth bundles. We demonstrate our method on several large real-world graphs.},
  author = {Ersoy, Ozan and Hurter, Christophe and Paulovich, Fernando V. and Cantareiro, Gabriel and Telea, Alexandru},
  doi = {10.1109/TVCG.2011.233},
  journal = {IEEE Transactions on Visualization and Computer Graphics},
  keywords = {paper_type:technique, group_type:edge-based, graph_vis:node-link, group_overlap:disjoint, group_structure:flat, edge-group_vis:visual_attr, group_vis:none, visual_attr:color, juxtaposed_vis:!, superimposed_vis:!, embedded_vis:!, implicit_vis:edge-bundling, graph:generic, group_origin:topology_clustering, evaluation:none, application:social_network, application:economy, publication_channel:TVCG, edge-bundling, circular_layout},
  number = {12},
  pages = {2364-2373},
  references = {Abello2006ASK-GraphView Archambault2007Grouse Gansner2007EdgeBundles Holten2006HierEdgeBundles vanHam2003CallMatrix},
  title = {Skeleton-Based Edge Bundling for Graph Visualization},
  volume = {17},
  year = {2011}
}

@InProceedings{Fagnan2012Community,
  abstract = {We present our COMmunity Boundary (COMB) and COMmunity Circles (COMC) network layout algorithms that focus on revealing the structure of discovered communities and the relationships between these communities. We believe this information is vital when developing new community mining algorithms as it allows the viewer to more quickly assess the quality of a mining result without appealing to large tables of statistics. To implement our algorithms we have introduced numerous modifications to the existing Fruchterman-Reingold layout, including support for multi-sized vertices, removal of the bounding frame, introduction of circular bounding boxes, and a novel slotting system. Our evaluation argues that both COMB and COMC outperform existing alternatives in their ability to reveal community structure and emphasize inter-community relations.},
  author = {Fagnan, Justin and Zaiane, Osmar R. and Goebel, Randy},
  booktitle = {Proceedings of the 16th International Conference on Information Visualisation},
  doi = {10.1109/IV.2012.61},
  issn = {1550-6037},
  keywords = {force-directed_layout, publication_channel:IV, edge-group_vis:none, group_vis:implicit, group_overlap:disjoint, group_structure:flat, paper_type:technique_layout, group_type:vertex-based,, graph_vis:node-link, implicit_vis:2D_layout, implicit_vis:2d_meta_layout, circular_layout, edge-group_vis:none, group_vis:visual_attr, visual_attr:color, evaluation:algorithmic, application:social_network, graph:generic, group_origin:generic, superimposed_vis:!, juxtaposed_vis:!, embedded_vis:!},
  pages = {321--330},
  publisher = {IEEE},
  references = {Balzer2007LoD-clust Frishman2004Clust},
  series = {IV '12},
  title = {Visualizing community centric network layouts},
  year = {2012}
}

@InProceedings{Fekete2003LinkOnTreemap,
  abstract = {Every graph can be decomposed into a tree structure plus a set of remaining edges. We describe a visualization technique that displays the tree structure as a Treemap and the remaining edges as curved links overlaid on the Treemap. Link curves are designed to show where the link starts and where it ends without requiring an explicit arrow that would clutter the already dense visualization. This technique is effective for visualizing structures where the underlying tree has some meaning, such as Web sites or XML documents with cross-references. Graphic attributes of the links – such as color or thickness – can be used to represent attributes of the edges. Users can choose to see all links at once or only the links to and from the node or branch under the cursor.},
  author = {Fekete, Jean-Daniel and Wang, Taowei David and Dang, N. and Aris, Aleks and Plaisant, Catherine},
  booktitle = {Proceedings of the 9th IEEE Symposium on Information Visualization, Poster Compendium},
  journal = {Poster compendium of IEEE Symposium on Information Visualization},
  keywords = {treemap, paper_type:technique, group_type:vertex-based, group_overlap:disjoint, group_structure:hierarchical, edge-group_vis:none, group_vis:superimposed, graph_vis:node-link, implicit_vis:2D_layout, graph:directed, group_origin:generic, evaluation:none, application:generic, publication_channel:poster_abstract, superimposed_vis:partitioning, juxtaposed_vis:!, visual_attr:!, embedded_vis:!},
  publisher = {IEEE},
  series = {InfoVis '03},
  title = {Overlaying Graph Links on Treemaps},
  year = {2003}
}

@InCollection{Feng1995PlanarCluster,
  abstract = {In this paper, we introduce and show how to draw a practical graph structure known as clustered graphs. We present an algorithm which produces planar, straight-line, convex drawings of clustered graphs in O(n2.5) time. We also demonstrate an area lower bound and an angle upper bound for straight-line convex drawings of C-planar graphs. We show that such drawings require omega(2n) area and the smallest angle is O(1/n). Our bounds are unlike the area and angle bounds of classical graph drawing conventions in which area bound is omega(n2) and angle bounds are functions of the maximum degree of the graph. Our results indicate important tradeoff between line straightness and area, and between region convexity and area.},
  author = {Feng, Qing-Wen and Cohen, Robert F. and Eades, Peter},
  booktitle = {Proceedings of the 1st Annual International Conference on Computing and Combinatorics},
  doi = {10.1007/BFb0030816},
  keywords = {paper_type:technique_layout, group_type:vertex-based,, graph_vis:node-link, group_overlap:disjoint, group_overlap:disjoint, group_structure:hierarchical, group_origin:topology_clustering, edge-group_vis:none, group_vis:implicit, implicit_vis:2D_layout, superimposed_vis:contour, graph:generic, evaluation:none, application:generic, publication_channel:COCOON, juxtaposed_vis:!, visual_attr:!, embedded_vis:!},
  pages = {21--30},
  publisher = {Springer},
  references = {Sugiyama1991CompoundDigraph},
  series = {COCOON '95},
  title = {How to draw a planar clustered graph},
  volume = {959},
  year = {1995}
}

@InProceedings{Fittkau2015SoftwareLandscape,
  abstract = {In many enterprises the number of deployed applications is constantly increasing. Those applications - often several hundreds - form large software landscapes. The comprehension of such landscapes is frequently impeded due to, for instance, architectural erosion, personnel turnover, or changing requirements. Therefore, an efficient and effective way to comprehend such software landscapes is required. The current state of the art often visualizes software landscapes via flat graph-based representations of nodes, applications, and their communication. In our ExplorViz visualization, we introduce hierarchical abstractions aiming at solving typical system comprehension tasks fast and accurately for large software landscapes. To evaluate our hierarchical approach, we conduct a controlled experiment comparing our hierarchical landscape visualization to a flat, state-of-the-art visualization. In addition, we thoroughly analyze the strategies employed by the participants and provide a package containing all our experimental data to facilitate the verifiability, reproducibility, and further extensibility of our results. We observed a statistically significant increase of 14 % in task correctness of the hierarchical visualization group compared to the flat visualization group in our experiment. The time spent on the system comprehension tasks did not show any significant differences. The results backup our claim that our hierarchical concept enhances the current state of the art in landscape visualization.},
  author = {Fittkau, Florian and Krause, Alexander and Hasselbring, Wilhelm},
  booktitle = {Proceedings of the 3rd IEEE Working Conference on Software Visualization},
  keywords = {paper_type:evaluation, graph_vis:node-link, group_type:vertex-based, group_overlap:disjoint, group_structure:hierarchical, edge-group_vis:none, group_vis:superimposed, visual_attr:!, superimposed_vis:contour, embedded_vis:!, implicit_vis:!, graph:generic, group_origin:generic, evaluation:user_study, application:software_engineering, publication_channel:VISSOFT, contour_filled, juxtaposed_vis:!},
  pages = {36--45},
  publisher = {IEEE},
  series = {VISSOFT '15},
  title = {Hierarchical Software Landscape Visualization for System Comprehension: A Controlled Experiment},
  year = {2015}
}

