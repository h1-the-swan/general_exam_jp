  pages = {211-216},
  publisher = {IEEE},
  references = {Elmqvist2008ZAME Hlawatsch2014AdjacencyLists Eades1996MVCG Holten2006HierEdgeBundles Abello2006ASK-GraphView Balzer2007LoD-clust Archambault2008GrouseFlocks Vehlow2014EvolComm},
  series = {IV '15},
  title = {{Adjasankey}: Visualization of huge hierarchical weighted and directed graphs},
  year = {2015}
}

