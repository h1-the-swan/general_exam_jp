  pages = {29-38},
  publisher = {SciTePress},
  references = {Balzer2007LoD-clust Byelas2009AOI Dinkla2012KelpDiagrams Gansner2010GMap Heer2005Vizster HenryRiche2010Euler Sprenger2000H-BLOB vanHam2004SmallWorldG},
  series = {IVAPP '14},
  title = {An Inverse Distance-based Potential Field Function for Overlapping Point Set Visualization.},
  year = {2014}
}

@InCollection{Wang1995CustomLayout,
  abstract = {A good layout tool should be able to generate customized layouts according to different requirements given by the user or applications. To achieve this goal, existing layout techniques should be enhanced and integrated to take their advantages while compensating their disadvantages. This paper presents three layout techniques based on the force-directed placement approach, including a revised force-directed placement to draw graphs with vertices of nontrivial sizes, a divide-andconquer approach to generate structured layouts, and an integrated approach to support constraints. The combination of the three techniques significantly improves the layout ability of the force-directed placement. They can be used to generate customized layouts that reflect semantics, preference, or principles of perceptual psychology.},
  author = {Wang, Xiaobo and Miyamoto, Isao},
  booktitle = {Proceedings of the 3rd International Symposium on Graph Drawing},
  doi = {10.1007/BFb0021835},
  keywords = {group_overlap:disjoint, group_structure:flat, graph_vis:node-link, publication_channel:GD, force-directed_layout, paper_type:technique_layout, group_type:vertex-based,, edge-group_vis:none, group_vis:implicit, implicit_vis:2D_meta_layout, implicit_vis:2D_layout, evaluation:none, graph:generic, group_origin:generic, application:generic, superimposed_vis:!, juxtaposed_vis:!, visual_attr:!, embedded_vis:!},
  pages = {504--515},
  publisher = {Springer},
